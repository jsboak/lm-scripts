            <name>scriptgroovy</name>
            <value>import org.openqa.selenium.chrome.*;
import org.openqa.selenium.interactions.*;
import org.openqa.selenium.*;

username = hostProps.get(&#34;username&#34;)
password = hostProps.get(&#34;password.pass&#34;)

System.setProperty(&#34;webdriver.chrome.driver&#34;, &#34;/usr/bin/chromedriver&#34;);
ChromeOptions options = new ChromeOptions();
// options.headless = true
options.addArguments(
    &#34;--headless&#34;,
    &#34;--disable-infobars&#34;,
    &#34;--disable-dev-shm-usage&#34;,
    &#34;--no-sandbox&#34;);
options.setBinary(&#34;/usr/bin/google-chrome&#34;);

WebDriver driver = new ChromeDriver(options);

try {
    driver.get(&#34;https://jake.logicmonitor.com/santaba/&#34;);    
    driver.findElement(By.id(&#34;username&#34;)).getText();
    
    println &#34;found_site=1&#34;
} catch (Exception e) {
    println &#34;found_site=0&#34;
}

try {   
    WebElement element = driver.findElement(By.id(&#34;username&#34;));
    Actions builder = new Actions(driver);
    builder.doubleClick(element).perform();
    
    driver.findElement(By.id(&#34;username&#34;)).click();
    driver.findElement(By.id(&#34;username&#34;)).sendKeys(username);
    driver.findElement(By.id(&#34;user-login-con&#34;)).click();
    driver.findElement(By.id(&#34;password&#34;)).click();
    
    element = driver.findElement(By.id(&#34;password&#34;));
    builder = new Actions(driver);
    builder.doubleClick(element).perform();
    
    driver.findElement(By.id(&#34;password&#34;)).sendKeys(password);
    driver.findElement(By.id(&#34;btn-login&#34;)).click();
    sleep(3000)
    driver.findElement(By.id(&#34;lnkEditProfile&#34;)).click();

    println &#34;logged_in=1&#34;
} catch (Exception e) {
    println &#34;logged_in=0&#34;
    println e
}

try {
    driver.findElement(By.id(&#34;lnkLogout&#34;)).click();
    driver.close()
    driver.quit()
    println &#34;logged_out=1&#34;
} catch (Exception e) {
    println &#34;logged_out=0&#34;
    println e
}

Runtime rt = Runtime.getRuntime();
Process proc = rt.exec(&#34;pkill chrome&#34;);

return 0;</value>
